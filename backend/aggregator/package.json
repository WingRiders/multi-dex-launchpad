{
  "name": "@wingriders/multi-dex-launchpad-aggregator",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsgo",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
