    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@cardano-ogmios/schema": "^6.14.0",
    "@types/bun": "^1.3.3",
    "@types/cors": "^2.8.19",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20251205.1",
    "prisma": "^7.2.0",
    "typescript": "^6.0.0-dev.20251205"
