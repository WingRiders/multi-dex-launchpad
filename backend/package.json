{
  "name": "@wingriders/multi-dex-launchpad-backend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "bun prisma:generate && tsgo",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "check": "bun biome check . && bun run tsgo --noEmit",
    "fix": "bun biome check . --write",
    "clean": "rm -r prisma/generated",
    "prisma:generate": "bunx --bun prisma generate --no-hints",
    "prisma:dev": "bunx --bun prisma migrate dev",
    "prisma:deploy": "bunx --bun prisma migrate deploy",
    "prisma:status": "bunx --bun prisma migrate status"
  },
  "dependencies": {
    "@cardano-ogmios/client": "^6.14.0",
    "@meshsdk/common": "^1.9.0-beta.93",
    "@meshsdk/core": "^1.9.0-beta.93",
    "@meshsdk/core-csl": "1.9.0-beta.93",
    "@meshsdk/core-cst": "^1.9.0-beta.93",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@trpc/server": "^11.7.2",
    "@wingriders/multi-dex-launchpad-common": "workspace:*",
    "better-result": "^2.3.1",
    "cors": "^2.8.5",
    "es-toolkit": "^1.44.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "superjson": "^2.2.2",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@cardano-ogmios/schema": "^6.14.0",
    "@types/bun": "^1.3.3",
    "@types/cors": "^2.8.19",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20251205.1",
    "typescript": "^6.0.0-dev.20251205"
  }
}
