{
  "name": "@wingriders/multi-dex-launchpad-backend-common",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./prisma": "./prisma/generated/client.ts"
  },
  "scripts": {
    "build": "bun prisma:generate && tsgo",
    "check": "bun biome check . && bun run tsgo --noEmit",
    "fix": "bun biome check . --write",
    "clean": "rm -r prisma/generated",
    "prisma:generate": "bun prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/bun": "^1.3.3",
    "@typescript/native-preview": "7.0.0-dev.20251205.1",
    "prisma": "^7.2.0",
    "typescript": "^6.0.0-dev.20251205"
  }
}
