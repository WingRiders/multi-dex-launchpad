{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "prisma:generate": {
      "inputs": ["prisma/schema.prisma"],
      "outputs": ["prisma/generated/**"]
    },
    "build": {
      "dependsOn": ["prisma:generate"],
      "outputs": ["dist"],
      "env": ["NODE_ENV"]
    },
    "check": {
      "dependsOn": ["prisma:generate"],
      "inputs": ["$TURBO_DEFAULT$", "$TURBO_ROOT$/biome.json"]
    },
    "fix": {
      "dependsOn": ["prisma:generate"],
      "inputs": ["$TURBO_DEFAULT$", "$TURBO_ROOT$/biome.json"]
    },
    "test": {},
    "clean": {}
  }
}
